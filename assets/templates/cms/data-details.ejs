<script id="template-data-details" type="text/template">
<div class="row span12">
	<div class="hero-unit">
		<h1>{{model.globalId}}s</h1>
		<p>{{data.length}} items</p>
	</div>
	<table class="table table-hover table-striped table-bordered">
		<thead>
		    <tr>
		      <th ng-repeat="attr in attributes">
		      	{{attr._key}}
		      </th>
		    </tr>
		</thead>
		<tbody>
	    <tr ng-repeat="row in data">

	      <td ng-repeat="attr in attributes">
	      	{{row[attr._key]}}
	      </td>
	    </tr>
	  </tbody>
	</table>

</div>
</script>